{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "moduleResolution": "node",
      "baseUrl": ".",
      "paths": {
        "@/*": ["src/*"]
      },
      "jsx": "preserve",
      "allowSyntheticDefaultImports": true
    },
    "include": ["src/**/*.js", "src/**/*.vue"],
    "exclude": ["node_modules"]
  }
  